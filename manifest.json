{
    "name": "Cusdis on Docker Compose",
    "id": "cusdis_dc",
    "packaging_format": 1,
    "description": {
        "en": "Cusdis is an open-source, lightweight (~5kb gzip), privacy-friendly alternative to Disqus.",
        "fr": "Cusdis est une alternative open-source, légère (~ 5kb gzip) et respectueuse de la vie privée à Disqus."
    },
    "version": "1.3.0~ynh1",
    "url": "https://cusdis.com",
    "upstream": {
        "license": "GNU GPLv3",
        "website": "https://cusdis.com",
        "demo": "https://cusdis.com",
        "admindoc": "https://cusdis.com/doc",
        "userdoc": "https://cusdis.com/doc",
        "code": "https://github.com/djyde/cusdis"
    },
    "license": "GNU GPLv3",
    "maintainer": {
        "name": "jarod5001",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/cusdis_dc",
                "default": "/cusdis_dc"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "password",
                "type": "password",
                "help": {
                    "en": "Password.",
                    "fr": "Mot de passe."
                }
            }
        ]
    }
}
